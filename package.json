{
	"name": "gherkintablealign",
	"displayName": "Gherkin Table Align",
	"description": "A table formatter for gherkin tables.",
	"version": "0.0.3",
	"publisher": "AlanCole",
	"repository": {
		"type": "git",
		"url": "https://github.com/alan-cole/vs-code-table-align"
	},
	"engines": {
		"vscode": "^1.38.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:alan-cole.gherkinTableAlign"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "alan-cole.gherkinTableAlign",
				"title": "Gherkin Table Align"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.17.35",
		"@types/vscode": "^1.49.0",
		"glob": "^7.1.6",
		"mocha": "^6.2.3",
		"tslint": "^5.20.1",
		"typescript": "^3.9.7",
		"vscode-test": "^1.4.0"
	}
}
